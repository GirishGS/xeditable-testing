<p id="notice"><%= notice %></p>

<h1>Listing Exms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exms.each do |exm| %>
      <tr>
        <td><%= exm.name %></td>
        <td><%= link_to 'Show', exm %></td>
        <td><%= link_to 'Edit', edit_exm_path(exm) %></td>
        <td><%= link_to 'Destroy', exm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exm', new_exm_path %>
